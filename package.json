{
  "name": "crawlab-docs",
  "version": "0.7.0",
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "pnpm@9.9.0",
  "scripts": {
    "dev": "pnpm -r run dev",
    "test": "pnpm -r run test",
    "build": "pnpm run build:packages && pnpm run merge-outputs",
    "build:packages": "pnpm -r --filter='./packages/*' run build",
    "build:parallel": "pnpm -r --parallel --filter='./packages/*' run build && pnpm run merge-outputs",
    "build:v06": "pnpm --filter='./packages/v0.6' run build",
    "build:v07": "pnpm --filter='./packages/v0.7' run build",
    "merge-outputs": "node scripts/merge-outputs.js",
    "clean": "pnpm -r run clean && rm -rf dist",
    "install:all": "pnpm install"
  },
  "devDependencies": {
    "fs-extra": "^11.2.0",
    "typescript": "^5.0.0"
  }
}